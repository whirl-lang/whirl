struct Test {
	a: int,
	b: String,
}

proc main() :: int {
	let c: int[] = [1, 2, 3, 4];
	let b: int = test_loops(c);

	test_structs();

	escape 0;
}

proc test_structs() :: void { 
	let a: Test = Test {
		a: 5,
		b: "Test"
	};
	
	println(a.a);
}

proc test_loops(arr: int[]) :: int {
	let a: int = 5;

	until a > 10 {
		a = a + 1;
	}

	iter i in 0..len(arr) {
		if i == 3 {
			escape 5;
		}
	}

	escape -1;
}
